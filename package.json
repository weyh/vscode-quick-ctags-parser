{
  "name": "quick-ctags-parser",
  "displayName": "quick-ctags-parser",
  "description": "A simple ctags file parser that let's you jump to the definition of a tag",
  "publisher": "weyh",
  "author": {
    "name": "weyh",
    "url": "https://github.com/weyh"
  },
  "repository": {
    "url": "https://github.com/weyh/vscode-quick-ctags-parser"
  },
  "version": "1.0.1",
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "quick-ctags-parser.jumpToDefinition",
        "title": "Jump to definition"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "quick-ctags-parser.jumpToDefinition",
          "title": "Jump to definition",
          "when": "editorTextFocus",
          "group": "navigation"
        }
      ]
    },
    "keybindings": [
      {
        "command": "quick-ctags-parser.jumpToDefinition",
        "key": "ctrl+t",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "npm run check-types && npm run lint && node esbuild.js",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "npm run check-types && npm run lint && node esbuild.js --production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.75.0",
    "@types/mocha": "^10.0.9",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.10.0",
    "@typescript-eslint/parser": "^8.7.0",
    "eslint": "^9.13.0",
    "esbuild": "^0.24.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.6.3",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}
